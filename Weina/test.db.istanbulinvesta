    "#!/usr/bin/python\n",
    "import sqlite3\n",
    "conn = sqlite3.connect('test.db')\n",
    "print \"Opened database successfully\";\n",
    "\n",
    "#Create table\n",
    "conn.execute('CREATE TABLE IstanbulInvesta (\n",
    "ID INT PRIMARY KEY NOT NULL,\n",
    "NAME TEXT NOT NULL,\n",
    "AGE INT NOT NULL,\n",
    "ADDRESS varchar(50),\n",
    "SALARY REAL');\n",
    "\n",
    "INSERT INTO IstanbulInvesta (\n",
    "ID,\n",
    "NAME,\n",
    "AGE,\n",
    "ADDRESS,\n",
    "SALARY REAL)\n",
    "\n",
    "VALUES(\n",
    "10001,\n",
    "'Weina',\n",
    "44,\n",
    "'152 Bulwara Road',\n",
    "110000)\n",
    "\n",
    "print \"Table created successfully\"; \n",
    "             \n",
    "conn.close()"
   ]
